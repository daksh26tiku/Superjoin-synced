{
  "version": 2,
  "builds": [
    {
      "src": "backend/public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/dashboard.html",
      "dest": "/backend/public/dashboard.html"
    },
    {
      "src": "/test-simple.html",
      "dest": "/backend/public/test-simple.html"
    },
    {
      "src": "/dashboard-backup.html",
      "dest": "/backend/public/dashboard-backup.html"
    },
    {
      "src": "/",
      "dest": "/backend/public/dashboard.html"
    }
  ]
}
